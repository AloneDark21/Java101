import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        int n;
        Scanner input = new Scanner(System.in);
        System.out.print("Bir Sayi Giriniz : ");
        n = input.nextInt();

        System.out.println(n+" e kadar olan 4 Ã¼n Kuvvetleri : ");
        for (int i=1;i<n;i*=4)
        {
            System.out.println(i);
        }
        System.out.println(n+" ya kadar olan 5 in Kuvvetleri : ");
        for (int i=1;i<n;i*=5)
        {
            System.out.println(i);
        }

    }
}
